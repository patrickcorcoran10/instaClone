{
  "name": "instaclone",
  "version": "1.0.0",
  "description": "Instagram Clone",
  "proxy": "http://localhost:3001/",
  "main": "server.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/patrickcorcoran10/instaClone.git"
  },
  "author": "pc",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/patrickcorcoran10/instaClone/issues"
  },
  "homepage": "https://github.com/patrickcorcoran10/instaClone#readme",
  "dependencies": {
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.19.0",
    "concurrently": "^5.1.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.0",
    "jsonwebtoken": "^8.5.1",
    "mysql2": "^2.1.0",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "path": "^0.12.7",
    "sequelize": "^5.21.5"
  }
}
